let
  fun fact n =
    if n = 0
    then 1
    else n * (fact (n - 1))
in
  fact 5
end;
val it = 120 : int

let
  fun sum x y = x + y
in
  sum 2
end;
val it = fn : int -> int

it 3;
val it = 5 : int
